# Instagram Bot Configuration
# Copy this file to .env and fill in your actual values

# Gemini AI API Key - Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Instagram Login Credentials
INSTAGRAM_USERNAME=your_instagram_username
INSTAGRAM_PASSWORD=your_instagram_password

# Target User to Chat With
TARGET_USERNAME=target_username_to_chat

# Optional: Bot Configuration
BOT_NAME=chube
MESSAGE_DELAY=3
IDLE_THRESHOLD=20

# Optional: Chrome Configuration
CHROME_HEADLESS=true
CHROME_WINDOW_SIZE=1920,1080

# Optional: Security Configuration
RATE_LIMIT_MESSAGES=10
RATE_LIMIT_WINDOW=60
SPAM_THRESHOLD=0.8
MAX_LOGIN_ATTEMPTS=3

# Optional: Performance Configuration
PERFORMANCE_CHECK_INTERVAL=30
HEALTH_SCORE_THRESHOLD=0.7
CPU_ALERT_THRESHOLD=80
MEMORY_ALERT_THRESHOLD=85
RESPONSE_TIME_THRESHOLD=5.0 